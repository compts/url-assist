{
  "name": "url-assist",
  "version": "1.1.0",
  "description": "HTTP base promised and callback and User agent",
  "main": "src/index.js",
  "module": "dist/esm/src/index.js",
  "types": "dist/ts/index.d.ts",
  "scripts": {
    "es_linter": "eslint src/lib/*.js src/*.js --fix",
    "test_esm": "mocha -r esm test/esm/*.test.js",
    "test_cjs": "mocha test/cjs/*.test.js",
    "test_ts": "mocha --require ts-node/register test/ts/*.test.ts",
    "validate": "npm run style && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/compts/url-assist.git"
  },
  "keywords": [
    "compt"
  ],
  "tsd": {
    "directory": "test/ts"
  },
  "author": "Pein Freccs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/compts/url-assist/issues"
  },
  "homepage": "https://urlassist.codehyouka.xyz",
  "dependencies": {
    "tsd": "^0.19.1",
    "structkit": "^1.4.2"
  },
  "devDependencies": {
    "@types/assert": "^1.5.6",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.4.4",
    "eslint": "^7.16.0",
    "eslint-config-strongloop": "^2.1.0",
    "eslint-plugin-import": "^2.22.1",
    "esm": "^3.2.25",
    "jest": "^27.5.1",
    "mocha": "^5.2.0",
    "ts-node": "^10.9.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "*.js",
      "*.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/test/esm/"

    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    }
  }
}
